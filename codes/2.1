#include <iostream>

using namespace std;

int main()
{
    int* B; 
    int size; 
    int dob;

    cout << "n = ";
    cin >> size;

    if (size <= 0) {

        cout << "Invalid size" << endl;
        return 1;
    }

    B = new int[size]; 

    for (int i = 0; i < size; i++) {
        cout << "B[" << i << "] = ";
        cin >> B[i];
    }

    int max = B[0];
    for (int i = 1; i < size; i++) {
        if (B[i] > max) {
            max = B[i];
        }
    }
    int min = B[0];
    for (int i = 0; i < size; ++i)
    {
        if (B[i] < min)
        {
            min = B[i];
        }
    }

    cout << "\n";
    cout << "max = " << max << endl;
    cout << "min = " << min << endl;

    dob = min * max;
    cout << "dob = " << dob << endl;

    for (int i = 0; i < size; i++)
        if (B[i] == min)
            B[i] = dob;

    cout << "\n";
    for (int i = 0; i < size; ++i)
        cout << "B[" << i << "]= " << B[i] << "\n";
    cout << endl;

    cout << "Direct by Hokage" << endl;
}
